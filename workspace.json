{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "player-stats-service",
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      },
      "component": {
        "style": "scss"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "scss"
      }
    }
  },
  "projects": {
    "array-fns": {
      "root": "libs/array-fns",
      "sourceRoot": "libs/array-fns/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/array-fns/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/array-fns"],
          "options": {
            "jestConfig": "libs/array-fns/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "game-models": {
      "root": "libs/game-models",
      "sourceRoot": "libs/game-models/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/game-models/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/game-models"],
          "options": {
            "jestConfig": "libs/game-models/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "game-rules-fns": {
      "root": "libs/game-rules-fns",
      "sourceRoot": "libs/game-rules-fns/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/game-rules-fns/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/game-rules-fns"],
          "options": {
            "jestConfig": "libs/game-rules-fns/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "game-rules-service": {
      "root": "apps/game-rules-service",
      "sourceRoot": "apps/game-rules-service/src",
      "projectType": "application",
      "prefix": "game-rules-service",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/game-rules-service",
            "main": "apps/game-rules-service/src/main.ts",
            "tsConfig": "apps/game-rules-service/tsconfig.app.json",
            "assets": ["apps/game-rules-service/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/game-rules-service/src/environments/environment.ts",
                  "with": "apps/game-rules-service/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "game-rules-service:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/game-rules-service/tsconfig.app.json",
              "apps/game-rules-service/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/game-rules-service/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/game-rules-service/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/game-rules-service"]
        }
      }
    },
    "game-ui-store": {
      "root": "libs/game-ui-store",
      "sourceRoot": "libs/game-ui-store/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/game-ui-store/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/game-ui-store"],
          "options": {
            "jestConfig": "libs/game-ui-store/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "hooks": {
      "root": "libs/hooks",
      "sourceRoot": "libs/hooks/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/hooks/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/hooks"],
          "options": {
            "jestConfig": "libs/hooks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "jsonfire": {
      "root": "libs/jsonfire",
      "sourceRoot": "libs/jsonfire/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/jsonfire/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/jsonfire"],
          "options": {
            "jestConfig": "libs/jsonfire/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "landing": {
      "root": "apps/landing",
      "sourceRoot": "apps/landing/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/landing/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "landing:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "landing:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/landing/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/landing"],
          "options": {
            "jestConfig": "apps/landing/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "landing-e2e": {
      "root": "apps/landing-e2e",
      "sourceRoot": "apps/landing-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/landing-e2e/cypress.json",
            "tsConfig": "apps/landing-e2e/tsconfig.e2e.json",
            "devServerTarget": "landing:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "landing:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/landing-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ok-scoring-mobile": {
      "root": "apps/ok-scoring-mobile",
      "sourceRoot": "apps/ok-scoring-mobile/src",
      "projectType": "application",
      "architect": {
        "start": {
          "builder": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx start ok-scoring-mobile"
          }
        },
        "run-ios": {
          "builder": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "builder": "@nrwl/react-native:bundle",
          "outputs": ["apps/ok-scoring-mobile/build"],
          "options": {
            "entryFile": "apps/ok-scoring-mobile/src/main.tsx",
            "platform": "ios",
            "bundleOutput": "dist/apps/ok-scoring-mobile/ios/main.jsbundle"
          }
        },
        "run-android": {
          "builder": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "builder": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/ok-scoring-mobile/android/app/build/outputs/bundle",
            "apps/ok-scoring-mobile/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "builder": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/ok-scoring-mobile/src/main.tsx",
            "platform": "android",
            "bundleOutput": "dist/apps/ok-scoring-mobile/android/main.jsbundle"
          }
        },
        "sync-deps": {
          "builder": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "ensure-symlink": {
          "builder": "@nrwl/react-native:ensure-symlink",
          "options": {}
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/ok-scoring-mobile/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ok-scoring-mobile"],
          "options": {
            "jestConfig": "apps/ok-scoring-mobile/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ok-scoring-mobile-e2e": {
      "root": "apps/ok-scoring-mobile-e2e",
      "sourceRoot": "apps/ok-scoring-mobile-e2e/src",
      "projectType": "application",
      "architect": {
        "build-ios": {
          "builder": "@nrwl/detox:build",
          "options": {
            "detoxConfiguration": "ios.sim.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "ios.sim.release"
            }
          }
        },
        "test-ios": {
          "builder": "@nrwl/detox:test",
          "options": {
            "detoxConfiguration": "ios.sim.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "ios.sim.release"
            }
          }
        },
        "build-android": {
          "builder": "@nrwl/detox:build",
          "options": {
            "detoxConfiguration": "android.emu.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "android.emu.release"
            }
          }
        },
        "test-android": {
          "builder": "@nrwl/detox:test",
          "options": {
            "detoxConfiguration": "android.emu.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "android.emu.release"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "apps/ok-scoring-mobile-e2e/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    },
    "ok-scoring-web": {
      "root": "apps/ok-scoring-web",
      "sourceRoot": "apps/ok-scoring-web",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/ok-scoring-web",
            "outputPath": "dist/apps/ok-scoring-web"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "ok-scoring-web:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "ok-scoring-web:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "ok-scoring-web:build:production"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ok-scoring-web"],
          "options": {
            "jestConfig": "apps/ok-scoring-web/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/ok-scoring-web/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "ok-scoring-web-e2e": {
      "root": "apps/ok-scoring-web-e2e",
      "sourceRoot": "apps/ok-scoring-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ok-scoring-web-e2e/cypress.json",
            "devServerTarget": "ok-scoring-web:serve",
            "tsConfig": "apps/ok-scoring-web-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ok-scoring-web:serve:production"
