          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/ok-scoring-web-e2e/**/*.{js,ts}"]
          }
        }
      }
