{
  "name": "game-service-py",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/game-service-py/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/game-service-py",
        "main": "apps/game-service-py/src/main.ts",
        "tsConfig": "apps/game-service-py/tsconfig.app.json",
        "assets": ["apps/game-service-py/src/assets"],
        "compiler": "tsc",
        "target": "node"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/game-service-py/src/environments/environment.ts",
              "with": "apps/game-service-py/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "game-service-py:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/game-service-py/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/game-service-py"],
      "options": {
        "jestConfig": "apps/game-service-py/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
