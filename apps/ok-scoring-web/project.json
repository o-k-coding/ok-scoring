{
  "name": "ok-scoring-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ok-scoring-web",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "command": "npm run build",
      "options": {
        "cwd": "apps/ok-scoring-web"
      }
    },
    "deploy": {
      "command": "npm run pages:deploy",
      "options": {
        "cwd": "apps/ok-scoring-web"
      }
    },
    "dev": {
      "command": "npm run dev",
      "options": {
        "cwd": "apps/ok-scoring-web"
      }
    },
    "serve": {
      "executor": "@nx/remix:serve",
      "options": {
        "command": "yarn start",
        "manual": true,
        "port": 4200
      }
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "command": "remix-serve build/index.js",
      "options": {
        "cwd": "apps/ok-scoring-web"
      }
    },
    "typecheck": {
      "command": "tsc",
      "options": {
        "cwd": "apps/ok-scoring-web"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../coverage/apps/ok-scoring-web"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "ok-scoring-web:build",
        "port": 4200
      }
    }
  }
}
