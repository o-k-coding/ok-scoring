{
  "name": "rules-service-go",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/rules-service-go/src/cmd/api",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "./tools/executors/go/serve:goServe"
    },
    "go-run": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "go run *.go"
          }
        ],
        "cwd": "apps/rules-service-go/src/cmd/api"
      }
    },
    "go-fmt": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "go fmt ./src/..."
          }
        ],
        "cwd": "apps/rules-service-go"
      }
    }
  },
  "tags": []
}
