{
  "implicitDependencies": {
    "workspace.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "ok-scoring",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "components-react-hooks": {
      "tags": []
    },
    "components-react-mobile": {
      "tags": []
    },
    "components-react-timer-provider": {
      "tags": []
    },
    "data-fauna": {
      "tags": []
    },
    "data-game-entities": {
      "tags": [],
      "implicitDependencies": ["data-game-models"]
    },
    "data-game-models": {
      "tags": []
    },
    "data-game-test-data": {
      "tags": []
    },
    "data-generate-uuid": {
      "tags": []
    },
    "data-sqlite-fns": {
      "tags": []
    },
    "features-game-rules-fns": {
      "tags": []
    },
    "features-game-ui-store": {
      "tags": []
    },
    "features-player-score-history-fns": {
      "tags": []
    },
    "landing": {
      "tags": []
    },
    "landing-e2e": {
      "tags": [],
      "implicitDependencies": ["landing"]
    },
    "ok-scoring-mobile": {
      "tags": []
    },
    "ok-scoring-mobile-e2e": {
      "tags": [],
      "implicitDependencies": ["ok-scoring-mobile"]
    },
    "ok-scoring-web": {
      "tags": []
    },
    "ok-scoring-web-e2e": {
      "tags": [],
      "implicitDependencies": ["ok-scoring-web"]
    },
    "player-stats-service": {
      "tags": []
    },
    "rules-service-go": {
      "tags": []
    },
    "utils-array-fns": {
      "tags": []
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "dev": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "start": [
      {
        "target": "build",
        "projects": "self"
      }
    ]
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "generators": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      },
      "component": {
        "style": "scss"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "scss"
      }
    }
  },
  "defaultProject": "player-stats-service"
}
