      }
    }
  },
  "projects": {
    "game-rules-service": {
      "tags": []
    },
    "jsonfire": {
      "tags": []
    },
    "landing": {
      "tags": []
    },
    "landing-e2e": {
      "tags": [],
      "implicitDependencies": ["landing"]
    },
    "ok-scoring-web": {
      "tags": []
    },
    "ok-scoring-web-client": {
      "tags": []
    },
    "ok-scoring-web-client-e2e": {
      "tags": [],
      "implicitDependencies": ["ok-scoring-web-client"]
    },
    "ok-scoring-web-e2e": {
      "tags": [],
      "implicitDependencies": ["ok-scoring-web"]
    },
    "player-stats-service": {
      "tags": []
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
